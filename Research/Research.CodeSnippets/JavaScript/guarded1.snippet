<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>guarded1</Title>
    <Author>ADA</Author>
    <Shortcut>guarded1</Shortcut>
    <Description>Code snippet for a function with gaurds foreach parameter</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>name</ID>
        <ToolTip>Function name</ToolTip>
        <Default>myfunction</Default>
      </Literal>
      <Literal>
        <ID>arg1</ID>
        <ToolTip>First parameter.</ToolTip>
        <Default>arg1</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript">
      <![CDATA[function $name$($arg1$) {
    if ($arg1$ == null) { throw new Error("Variable [$arg1$] is null or undefined."); }
    
}]]></Code>
  </Snippet>
</CodeSnippet>